<%
  const { CodeGenerator } = require('E:\\Projects\\UML_code_generator\\.ejs\\code-generator.js');
  let header = CodeGenerator(element);
-%>
<%# Header guard start -%>
#ifndef <%=header.WriteHeaderGuard()%>
#define <%=header.WriteHeaderGuard()%>

<%# Include headers -%>
<%-header.WriteIncludes();%>

<%# Class declaration -%>
class <%=header.WriteName()%><%=header.WriteInheritance()%> {
<%#= 'public:\n\t' + Attributes('public', header.attributes) + Enumerations('public', header.enumerations)
                  + Methods('public', header.methods)-%>

<%#= 'protected:\n\t' + Attributes('protected', header.attributes)
                  + Enumerations('protected', header.enumerations) + Methods('protected', header.methods)-%>

<%#= 'private:\n\t' + Attributes('private', header.attributes) + Enumerations('private', header.enumerations)
                  + Methods('private', header.methods)-%>
};

<%# Header guard end -%>
#endif //<%=header.WriteHeaderGuard()%>

/* LOG:
  <%=header.log.join('\n')%>
*/