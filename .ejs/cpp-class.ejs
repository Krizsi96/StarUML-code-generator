<%
  const { CodeGenerator, CreateLogFile } = require('E:\\Projects\\UML_code_generator\\.ejs\\code-generator.js');
  let header = CodeGenerator(element);
-%>
<%# Header guard start -%>
#ifndef <%=header.WriteHeaderGuard()%>
#define <%=header.WriteHeaderGuard()%>

<%# Include headers -%>
<%-header.WriteIncludes();%>

<%# Class declaration -%>
class <%=header.WriteName()%><%=header.WriteInheritance()%> {
<%= 'public:\n\t' + header.WritePublicElements()-%>
<%= 'protected:\n\t' + header.WriteProtectedElements()-%>
<%= 'private:\n\t' + header.WritePrivateElements()-%>
};

<%# Header guard end -%>
#endif //<%=header.WriteHeaderGuard()%>
<%CreateLogFile(header)-%>